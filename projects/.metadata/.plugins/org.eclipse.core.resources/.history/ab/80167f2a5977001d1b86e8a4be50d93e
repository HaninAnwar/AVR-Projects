static Task_t SystemTasks[TASK_NUM] = {{NULL}};

void RTOS_voidStart(void)
{
	TIMER_u8SetCallBack(TIMER0_OUTPUT_COM_INT,&voidScheduler);
	GIE_voidEnable();
	TIMER0_u8Init(CTC,DISCONNECT,TIMER_DIV_BY_64);
}
