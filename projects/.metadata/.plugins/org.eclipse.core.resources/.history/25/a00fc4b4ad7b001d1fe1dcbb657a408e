#ifndef  RTOS_PRIVATE_H_
#define  RTOS_PRIVATE_H_

#define TASK_RESUMED		1
#define TASK_SUSPENDED		0

typedef struct
{
	u16 Periodicity;
	void (*TaskFunc)(void);
	u8 State;
}Task_t;

static void voidScheduler(void);

