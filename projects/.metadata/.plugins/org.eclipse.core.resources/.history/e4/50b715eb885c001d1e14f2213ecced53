#ifndef  TWI_REGISTER_H_
#define  TWI_REGISTER_H_

#define TWCR			*((volatile u8*)0x56)

#define TWCR_TWINT		7
#define TWCR_TWEA

#define TWDR			*((volatile u8*)0x23)		/*Data register*/

#define TWAR			*((volatile u8*)0x22)

#define TWSR			*((volatile u8*)0x21)

#define TWBR			*((volatile u8*)0x20)
#endif
