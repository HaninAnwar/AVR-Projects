#ifndef  SDD_INTERFACE_H_
#define  SDD_INTERFACE_H_


#define SDD_u8COM_ANODE		1
#define SDD_u8COM_CATHODE	0

#define SDD_u8NUM0			0
#define SDD_u8NUM1			1
#define SDD_u8NUM2			2
#define SDD_u8NUM3			3
#define SDD_u8NUM4			4
#define SDD_u8NUM5			5
#define SDD_u8NUM6			6
#define SDD_u8NUM7			7
#define SDD_u8NUM8			8
#define SDD_u8NUM9			9

typedef struct
{
	u8 Port;
	u8 EnablePort;
	u8 EnablePin;
	u8 ComType;

}SDD_T;


u8 SDD_u8SetNumber(u8 Copy_u8Number , SDD_T *SDD);

#endif
