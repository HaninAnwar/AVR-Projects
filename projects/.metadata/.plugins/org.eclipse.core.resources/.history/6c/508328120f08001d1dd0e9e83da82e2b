#ifndef  SW_INTERFACE_H_
#define  SW_INTERFACE_H_

#define LED_u8SOURCE    	1
#define LED_u8SINK    		0

typedef struct
{
	u8 Port;
	u8 Pin;
}SW_T;



u8 LED_u8TurnOn(LED_T *LED);
u8 LED_u8TurnOff(LED_T *LED);
u8 LED_u8Blink(u8 Copy_u8Delay ,LED_T *LED);


#endif
