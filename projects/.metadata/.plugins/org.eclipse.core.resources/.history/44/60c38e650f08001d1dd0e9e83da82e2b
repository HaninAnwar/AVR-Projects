#ifndef  SW_INTERFACE_H_
#define  SW_INTERFACE_H_

#define SW_Pressed			1
#define SW_NotPressed    	0

typedef struct
{
	u8 Port;
	u8 Pin;
}SW_T;

u8 SW_u8GetState(SW_T *SW);

